
@{
    ViewBag.Title = "Orden";
    Layout = "~/Views/Shared/_LayoutDash.cshtml";
}

<div id="page-wrapper">
    <div id="page-inner">
        <div class="row">
            <div class="col-md-12">
                <h1 class="page-header">
                    <small>@ViewBag.usuario</small>
                </h1>
            </div>
        </div>

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="row">
            <div class="col-md-12">
                <label class="col-form-label" style="color:red">Al realizar tu deposito de pago, sube tu comprobante en el apartado de "Mis Compras"</label>
            </div>
            <div class="col-md-12">
                <iframe src="@ViewBag.url" width="100%" height="600px"></iframe>
            </div>
            <div class="col-md-12">
            </div>
        </div>
    </div>
</div>